{
  "name": "dhcc/om-openapi",
  "type": "library",
  "description": "OM-OPENAPI SDK for PHP7",
  "keywords": [
    "丝路",
    "om",
    "openapi",
    "sdk",
    "api"
  ],
  "homepage": "https://github.com/dhcc-repo/om-openapi.git",
  "license": "MIT",
  "authors": [
    {
      "name": "dhcc-repo",
      "email": "dhcc-repo@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "guzzlehttp/psr7": "^1.4",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-openssl": "*"
  },
  "suggest": {
    "guzzlehttp/guzzle": "经典的兼容PSR7的HTTP客户端"
  },
  "autoload": {
    "psr-4": {
      "Dhcc\\OmOpenapi\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "<9.0.0"
  }
}
